<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.DriveToMSU" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/msu_red</item>
        <item name="colorPrimaryVariant">@color/msu_red_dark</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/msu_grey_light</item>
        <item name="colorSecondaryVariant">@color/msu_grey_dark</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    </style>
</resources>